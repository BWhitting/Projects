class Pet {
    private String name;

    // Constructor
    private Pet() {
        setName("Pet Name");
    }

    // Set method
    private void setName(String name) {
        this.name = name;
    }

    // Get method
    private String getName() {
        return name;
    }

    // toString method

    public String toString() {
        return "Pet information:\nName: " + name;
    }

    public static void main(String[] args) {
        Pet pet1 = new Pet();
        System.out.println(pet1);

        Pet pet2 = new Pet();
        pet2.setName("Bella");
        System.out.println(pet2);
    }
}
